{
  "name": "@llm-rate-limiter/e2e-test-runner",
  "version": "1.0.0",
  "description": "E2E test runner that sends jobs to the proxy server",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "rm -rf dist tsconfig*.tsbuildinfo && tsc -p tsconfig.build.json",
    "typecheck": "tsc -b",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts"
  },
  "private": true,
  "license": "MIT",
  "dependencies": {
    "@llm-rate-limiter/core": "^1.0.0",
    "@llm-rate-limiter/e2e-test-results": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
